{
    "name": "xt/laravel-inertia-element-ui-crud",
    "description": "Generate CRUD files for element ui + inertia js + Vue3",
    "keywords": [
        "xt",
        "laravel",
        "element-ui",
        "element-plus",
        "inertiajs",
        "inertia",
        "element-ui-crud"
    ],
    "homepage": "https://github.com/xt/element-ui-crud",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Hiren Reshamwala",
            "email": "hiren.reshamwala@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "doctrine/dbal": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "XT\\ElementUiCrud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XT\\ElementUiCrud\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "XT\\ElementUiCrud\\ElementUiCrudServiceProvider"
            ]
        }
    }
}
